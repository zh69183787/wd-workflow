<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.wonders.send.model.bo.TMsgUserMassage"
        table="T_MSG_USERMESSAGE"
        dynamic-update="true"
    >

        <id name="sid" type="java.lang.Long">
            <column name="SID" />
            <generator class="native">
                <param name="sequence">SEQ_T_MSG</param>
            </generator>
        </id>          
		<property name="empidrec" type="java.lang.Long">
            <column name="EMPIDREC" />
        </property>
        <property name="empidsend" type="java.lang.Long">
            <column name="EMPIDSEND" />
        </property>
        <property name="sendmode" type="java.lang.Long">
            <column name="SENDMODE" />
        </property>
        <property name="parentid" type="java.lang.Long">
            <column name="PARENTID" />
        </property>
        <property name="state" type="java.lang.Integer">
            <column name="STATE" />
        </property>
        <property name="autoalert" type="java.lang.Integer">
            <column name="AUTOALERT" />
        </property>
        <property name="msgcount" type="java.lang.Integer">
            <column name="MSGCOUNT" />
        </property>
        <property name="sendstate" type="java.lang.String">
            <column name="SENDSTATE" length="1"/>
        </property>
        <property name="recrstate" type="java.lang.String">
            <column name="RECRSTATE" length="1"/>
        </property>
        <property name="senddelete" type="java.lang.String">
            <column name="SENDDELETE" length="1"/>
        </property>
        <property name="recrdelete" type="java.lang.String">
            <column name="RECRDELETE" length="1"/>
        </property>
        <property name="title" type="java.lang.String">
            <column name="TITLE" length="1000"/>
        </property>
        <property name="content" type="java.lang.String">
            <column name="CONTENT" length="4000"/>
        </property>
        <property name="senddate" type="java.lang.String">
            <column name="SENDDATE" length="19"/>
        </property>
        <property name="seedate" type="java.lang.String">
            <column name="SEEDATE" length="19"/>
        </property>
        <property name="eventId" type="java.lang.Long">
            <column name="EVENT_ID" />
        </property>
        
    </class>

</hibernate-mapping>
